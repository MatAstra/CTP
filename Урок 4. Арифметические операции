>>> 20+2
22
>>> a=2
>>> b=2
>>> c=20
>>> a+b+c
24

>>> a=2
>>> b=20
>>> c=a+b
>>> print(c)
22

>>> 4-2
2
>>> a=7
>>> b=7
>>> a-b
0

>>> 10*4
40
>>> a=4
>>> a*=2
>>> print(a)
8

>>> 9/3
3.0
>>> a=8
>>> b=4
>>> a/b
2.0

>>> 5**5
3125
>>> a=5
>>> d=2
>>> a**d
25

>>> m=46
>>> hex(m)
'0x2e'
>>> oct(m)
'0o56'
>>> bin(m)
'0b101110'

>>> m=0b101110
>>> bin(m)
'0b101110'
>>> hex(m)
'0x2e'
